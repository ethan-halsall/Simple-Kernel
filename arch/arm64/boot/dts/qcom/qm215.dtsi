#include "msm8917.dtsi"
#include "qm215-camera.dtsi"
/ {
	model = "Qualcomm Technologies, Inc. QM215";
	compatible = "qcom,qm215";
	qcom,msm-id = <386 0x0>;
	qcom,msm-name = "QM215";
};

/ {
	qrd_batterydata: qcom,batterydata {
		qcom,rpull-up-kohm = <100>;
		qcom,vref-batt-therm = <1800000>;

		#include "vbms-batterydata-mlp356477-2800mah.dtsi"
	};
};

&soc {
	/delete-node/ devfreq-cpufreq;
	devfreq-cpufreq {
		cpubw-cpufreq {
		target-dev = <&cpubw>;
		cpu-to-dev-map =
			<  998400  4248 >,
			< 1094400  4541 >,
			< 1305600  5645 >;
		};

		mincpubw-cpufreq {
			target-dev = <&mincpubw>;
			cpu-to-dev-map =
				<  998400 2270 >,
				< 1305600 4248 >;
		};
	};
};

&soc {
	qcom,vidc@1d00000 {
		qcom,allowed-clock-rates = <308570000 308570000
					308570000 270000000 200000000>;
	};
};
